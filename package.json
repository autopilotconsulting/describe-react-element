{
  "name": "describe-react-element",
  "version": "0.1.0",
  "description": "Textualize react elements for testing and debugging",
  "main": "./distribution/index.js",
  "scripts": {
    "test": "jest --coverage --colors source",
    "build": "babel source --out-dir distribution --ignore **/*/__tests__/*",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autopilotconsulting/describe-react-element.git"
  },
  "keywords": [
    "react",
    "component",
    "reactjs",
    "describe",
    "string",
    "pretty",
    "print"
  ],
  "author": {
    "name": "D. Patrick Caldwell",
    "email": "tncbbthositg@gmail.com",
    "url": "http://www.autopilotconsulting.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/autopilotconsulting/describe-react-element/issues"
  },
  "homepage": "https://github.com/autopilotconsulting/describe-react-element#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^20.0.4",
    "jest-environment-node-debug": "^2.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "array-flatten": "^2.1.1"
  }
}
